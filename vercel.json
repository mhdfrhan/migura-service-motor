{
	"version": 2,
	"framework": null,
 
	"buildCommand": "npm install && npm run build",
	"outputDirectory": "public",
 
	"functions": {
	  "api/index.php": {
		 "runtime": "vercel-php@0.9.0"
	  }
	},
 
	"routes": [
	  {
		 "src": "^/build/(.*)",
		 "dest": "/public/build/$1"
	  },
	  {
		 "src": "^/(.*)",
		 "dest": "/api/index.php"
	  }
	]
 }
 